  "tokenColors": [
    {
      "name": "[COMMENTS]",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#526176"
      }
    },
    {
      "name": "[String]",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#a3be8c"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#d6d6d7"
      }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#b48ead",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Object Properties and Language Support Properties",
      "scope": [
        "variable.other.object.property",
        "variable.other.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#FFEE80"
      }
    },
    {
      "name": "Constants and Language Constants",
      "scope": [
        "variable.other.constant",
        "constant.language"
      ],
      "settings": {
        "foreground": "#F4A261",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#90c7ac"
      }
    },
    {
      "name": "Function and Method Declarations",
      "scope": [
        "entity.name.function",
        "meta.function.declaration .entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#86ABDC"
      }
    },
    {
      "name": "Function and Method Calls",
      "scope": [
        "meta.function-call .entity.name.function",
        "entity.name.function-call"
      ],
      "settings": {
        "foreground": "#88c0d0"
      }
    },
    {
      "name": "Control Keywords (if, for, return...)",
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "foreground": "#9D79D6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage & Declaration Keywords (function, var, class...)",
      "scope": [
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#BAA1E2"
      }
    },
    {
      "name": "Operators (in, of, new, await...)",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#AEAFB0"
      }
    },
    {
      "name": "Types and Classes (NO ITALIC)",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "foreground": "#ebcb8b"
      }
    },
    {
      "name": "Annotations & Decorators",
      "scope": [
        "punctuation.definition.decorator",
        "meta.decorator storage.type",
        "meta.decorator entity.name.function",
        "meta.decorator entity.name.type",
        "storage.type.annotation"
      ],
      "settings": {
        "foreground": "#93ccdc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#a3be8c"
      }
    },
    {
      "name": "Doc Comments and Tags",
      "scope": [
        "comment.block.documentation",
        "comment.line.documentation"
      ],
      "settings": {
        "foreground": "#6c7a8f",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSDoc/TSDoc Tags",
      "scope": "comment.block.documentation storage.type.class.jsdoc",
      "settings": {
        "foreground": "#9D79D6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "HTML/XML Tags",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#81a1c1"
      }
    },
    {
      "name": "HTML/XML Tag Brackets",
      "scope": [
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#526176"
      }
    },
    {
      "name": "HTML/XML Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#9D79D6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSON Property Names",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#81a1c1"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css",
        "entity.name.pseudo-class.css",
        "entity.name.pseudo-element.css"
      ],
      "settings": {
        "foreground": "#81a1c1"
      }
    },
    {
      "name": "CSS Property Names",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#d6d6d7"
      }
    },
    {
      "name": "CSS Property Values",
      "scope": [
        "support.constant.color.w3c-standard-color-name.css",
        "support.constant.font-name.css",
        "support.constant.media-type.css"
      ],
      "settings": {
        "foreground": "#a3be8c"
      }
    },
    {
      "name": "CSS Units",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#F4A261"
      }
    }
  ]
}
